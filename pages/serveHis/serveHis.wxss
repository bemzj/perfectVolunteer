/* pages/beforeComplete/beforeComplete.wxss */
page{
  width: 100%;
  height: 100%;
}
.wrap{
  width: 100%;
  height: 100%;
  background-color: #eaf9fa;
  padding-top: 50rpx;
}
.list{
  width: 85%;
  background-color: #ffffff;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 5px 10px rgba(222,222,222,0.4);
  margin-bottom: 40rpx;
}
.list >view{
  text-align: center;
}
.list >view:first-child{
  width: 70%;
  font-size: 24rpx;
  text-align: left;
  padding-left: 45rpx;
  display: flex;
  align-items: center;
  padding: 25rpx 0 25rpx 20rpx;
}
.list >view:first-child >view:first-child{
  width: 115rpx;
  height: 115rpx;
  border: 1px solid #55683f;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 20rpx;
}
.list >view:first-child >view:last-child{
  font-size: 24rpx;
  color: #383837;
}
.list >view:first-child >view:last-child >view{
  margin-bottom: 10rpx;
}
.list >view:first-child >view:last-child view:first-child{
  font-size: 33rpx;
}
.list >view:first-child >view:first-child image{
  width: 100%;
  height: 100%;
}
.list >view:last-child{
  width: 30%;
  background-color: #8fe8f4;
  color: #fff;
  font-size: 30rpx;
  padding: 30rpx 0;
  margin-right: 20rpx;
}
.list >view:last-child text{
  display: block;
}
