/* pages/completed/completed.wxss */
/* pages/beforeComplete/beforeComplete.wxss */
page{
  width: 100%;
  height: 100%;
}
.wrap{
  width: 100%;
  height: 100%;
  background-color: #eaf9fa;
  padding-top: 50rpx;
}
.list{
  width: 85%;
  background-color: #ffffff;
  margin: auto;
  display: flex;
  justify-content: center;
  align-items: center;
  box-shadow: 0 5px 10px rgba(222,222,222,0.4);
  margin-bottom: 40rpx;
}
.list >view{
  text-align: center;
  padding: 35rpx 0;
}
.list >view:first-child{
  width: 70%;
  font-size: 24rpx;
  text-align: left;
  padding-left: 45rpx;
  display: flex;
  align-items: center;
  padding: 0 0 0 20rpx;
}
.list >view:first-child >view:first-child{
  width: 115rpx;
  height: 115rpx;
  border: 1px solid #55683f;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 25rpx;
}
.list >view:first-child >view:last-child{
  font-size: 24rpx;
  color: #383837;
}
.list >view:first-child >view:last-child view:first-child{
  font-size: 33rpx;
  font-weight: bold;
  margin-bottom: 15rpx;
}
.list >view:first-child >view:first-child image{
  width: 100%;
  height: 100%;
}
.list >view:last-child{
  width: 30%;
  color: #908f8f;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.list >view:last-child text{
 font-size: 23rpx;
}
.list >view:last-child icon{
  width: 30rpx;
  height: 35rpx;

}