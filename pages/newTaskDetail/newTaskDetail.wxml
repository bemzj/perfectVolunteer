<!--pages/newTaskDetail/newTaskDetail.wxml-->
<view class='wrap' style='background:url("../../images/bj.png") no-repeat;background-size: 100% 100%;'>
  <view class='list'>
    <view class='list1'>
      <view>
        <image mode='aspectFit' src='../../images/bj03.png'></image>
      </view>
    </view>
    <view class='list2' style='background:url("../../images/bj04.png") no-repeat;background-size: 100% 100%;'>
      <view>
        <view>
          <view class='name'>
            <text>{{detailData.name == ''?'用户':detailData.name}}</text>
          </view>
          <view>
            <text>联系电话：{{detailData.phone == ''?'000-0000-0000':detailData.phone}}</text>
          </view>
          <view>
            <text>预约时间：{{detailData.date == ''?'2000年5月20日，13-15时':detailData.date}}</text>
          </view>
          <view>
            <text>服务地址：{{detailData.address == ''?'地址':detailData.address}}</text>
          </view>
          <view>
            <text>服务事项：{{detailData.matter == ''?'空调':detailData.matter}}</text>
          </view>
        </view>
      </view>
    </view>
    <view class='list3'>
      <view>拒绝</view>
      <view>接受</view>
    </view>
  </view>
</view>